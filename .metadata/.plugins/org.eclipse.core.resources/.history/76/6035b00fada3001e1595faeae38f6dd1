	public static void main(String[] args) {
		
		// 정수형
		byte num1 = 127;
		short num2 = 32767;
		int num3 = 2147483647;
		long num4 = 922337203685477507L;
		
		System.out.println("num1 : " + num1);
		System.out.println("num1 : " + num2);
		System.out.println("num1 : " + num3);
		System.out.println("num1 : " + num4);
	
		// 실수형
		
		
		
